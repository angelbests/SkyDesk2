        "maximizable": false,
        "visible": false
      },
      {
        "x": 99999999999,
        "y": 99999999999,
        "width": 80,
        "height": 45,
        "shadow": false,
        "decorations": false,
        "transparent": true,
        "url": "/#/pages/desktop/taskbar",
        "title": "taskbar",
        "label": "taskbar",
        "skipTaskbar": true,
        "resizable": false,
        "minimizable": false,
        "maximizable": false,
        "visible": false
      },
      {
        "x": 999999999999,
        "y": 999999999999,
        "width": 240,
        "height": 240,
        "shadow": false,
        "decorations": false,
        "transparent": true,
        "alwaysOnTop": true,
        "url": "/#/pages/desktop/wheel",
        "title": "wheel",
        "label": "wheel",
        "skipTaskbar": true,
        "resizable": false,
        "minimizable": false,
        "maximizable": false,
        "visible": false
