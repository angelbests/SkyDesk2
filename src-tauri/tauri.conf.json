{
  "productName": "skydesk2",
  "version": "1.3.4",
  "identifier": "com.skydesk2.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:24221",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "SkyDesk2",
        "width": 900,
        "minWidth": 900,
        "height": 650,
        "minHeight": 650,
        "shadow": false,
        "decorations": false,
        "label": "main",
        "fullscreen": false,
        "transparent": true,
        "center": true,
        "dragDropEnabled": false,
        "alwaysOnTop": false,
