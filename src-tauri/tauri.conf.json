        "height": 100,
        "shadow": false,
        "decorations": false,
        "transparent": true,
        "alwaysOnTop": true,
        "url": "/#/pages/desktop/tray",
        "title": "tray",
        "label": "tray",
        "skipTaskbar": true,
        "resizable": false,
        "minimizable": false,
        "maximizable": false,
        "visible": false
      },
      {
        "x": 0,
        "y": 0,
        "width": 80,
        "height": 45,
        "shadow": true,
        "decorations": false,
        "transparent": true,
