      {
        "width": 800,
        "height": 50,
        "decorations": false,
        "transparent": true,
        "dragDropEnabled": false,
        "shadow": false,
        "maximizable": false,
        "resizable": true,
        "skipTaskbar": true,
        "center": true,
        "alwaysOnTop": true,
        "url": "/#/pages/desktop/search",
        "title": "search",
        "label": "search",
        "visible": false
      }
    ],
    "security": {
      "csp": null,
      "assetProtocol": {
        "enable": true,
        "scope": [
          "$APPDATA/**",
          "$RESOURCE/**",
          "$VIDEO/**",
          "$PICTURE/**"
        ]
      }
    },
    "withGlobalTauri": true
  },
  "bundle": {
    "publisher": "angelbests1314@live.com",
    "active": true,
    "targets": [
      "msi",
      "nsis"
    ],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "externalBin": [],
    "resources": [
      "icons/icon.ico",
      "resources/*"
    ],
    "fileAssociations": [
      {
        "ext": [
          "png"
        ]
      }
    ],
    "windows": {
      "nsis": {
        "installerIcon": "./icons/icon.ico",
        "displayLanguageSelector": true,
        "languages": [
          "SimpChinese"
        ]
      },
      "wix": {
        "language": "zh-CN",
        "bannerPath": "./resources/bg.png"
      }
    },
    "homepage": "https://github.com/angelbests/SkyDesk2",
    "shortDescription": "一个简单易用的本地工具",
    "longDescription": "壁纸：动态壁纸、网页壁纸、静态壁纸、锁屏壁纸、音乐可视化（支持桌面切换音乐和启停）\n快捷：轮盘快速启动应用，桌面应用合集卡片\n便签：简单易编辑，可自定义颜色、置顶方式、透明度、嵌入到桌面鼻子\nAI:支持ollama部署的模型进行对话\n录屏：支持自定义绘制录屏画面大小（无声音）\n日历：简单的日历\n",
    "category": "Utility",
    "copyright": "SkyDesk2",
    "createUpdaterArtifacts": true
  },
  "plugins": {
    "updater": {
      "active": true,
      "window": {
